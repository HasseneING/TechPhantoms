      <ComboBox fx:id="startHourCombo" layoutX="465.0" layoutY="301.0" prefHeight="26.0" prefWidth="78.0" />
      <ComboBox fx:id="startMinCombo" layoutX="592.0" layoutY="301.0" prefHeight="26.0" prefWidth="78.0" />
      <ComboBox fx:id="endHourCombo" layoutX="465.0" layoutY="350.0" prefHeight="26.0" prefWidth="78.0" />
      <ComboBox fx:id="endMinCombo" layoutX="592.0" layoutY="350.0" prefHeight="26.0" prefWidth="78.0" />

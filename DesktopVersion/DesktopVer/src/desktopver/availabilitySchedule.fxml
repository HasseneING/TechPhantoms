            <Font size="27.0" />
         </font>
      </Label>
      <TableView fx:id="scheduleView" layoutX="116.0" layoutY="71.0" prefHeight="200.0" prefWidth="550.0">
        <columns>
          <TableColumn fx:id="dateIDCol" prefWidth="75.0" text="dateID" />
          <TableColumn fx:id="startTimeCol" prefWidth="75.0" text="startTime" />
            <TableColumn fx:id="endTimeCol" prefWidth="75.0" text="endTime" />
            <TableColumn fx:id="availableCol" prefWidth="75.0" text="available" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
